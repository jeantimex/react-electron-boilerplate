{
  "name": "react-electron-boilerplate",
  "description": "React Electron boilerplate",
  "version": "0.1.0",
  "author": {
    "name": "Yong Su",
    "email": "jean.timex@gmail.com",
    "url": "https://github.com/jeantimex"
  },
  "scripts": {
    "start": "electron app/dist/main.bundle.js",
    "clean": "rimraf app/dist",
    "dev:main": "webpack --config conf/webpack/webpack.config.main.babel.js",
    "dev:renderer": "webpack-dev-server --config conf/webpack/webpack.config.renderer.babel.js",
    "dev": "nf start -p 3000",
    "prod:renderer": "cross-env NODE_ENV=production webpack-dev-server --config conf/webpack/webpack.config.renderer.babel.js --env.prod=true",
    "build:renderer": "cross-env NODE_ENV=production webpack --config conf/webpack/webpack.config.renderer.babel.js --env.prod=true",
    "build:main": "cross-env NODE_ENV=production webpack --config conf/webpack/webpack.config.main.babel.js --env.prod=true",
    "build": "npm run clean && npm run build:main && npm run build:renderer"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "electron": "^1.6.10",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "foreman": "^2.0.0",
    "node-sass": "^4.5.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
